	    <div role="tabpanel" class="tab-pane" id="games">
				<div class="col-md-4">
		    	<div class="panel panel-default post-panel">
		    		<%= link_to "http://vim-adventures.com/", target: '_blank' do %>
		    		<div class="panel-body post-panel-body learn-info">
	    				VIM Adventures
					  </div>
					  <% end %>
					</div>
	    	</div>
	    </div>
	    <!--Reference tab pane-->
