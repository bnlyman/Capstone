		    <li role="presentation"><a href="#reference" aria-controls="Other Coding" role="tab" data-toggle="tab">Reference</a></li>
		    <li role="presentation"><a href="#videosTab" aria-controls="Other Coding" role="tab" data-toggle="tab">Videos</a></li>
	  	</ul>
	  </div>
	  <!-- Tab panes -->
	  <div class="tab-content">
	  	<!--Courses tab pane-->
	    <div role="tabpanel" class="tab-pane active" id="courses">
		    <div class="col-md-4">
			    <div class="panel panel-default post-panel ">
			    	<%= link_to "http://www.codelearn.org", target: '_blank' do %>
			    	<div class="panel-body post-panel-body learn-info">
			    		CodeLearn
			    	</div>
			    	<% end %>
					</div>
			    <div class="panel panel-default post-panel">
			    		<%= link_to "https://teamtreehouse.com/library/build-a-simple-ruby-on-rails-application/getting-started-with-rails/generate-a-rails-application-2", target: '_blank' do %>
			    		<div class="panel-body post-panel-body learn-info">
			    			Treehouse
			   	  </div>
			   	  <% end %>
					</div>
			   	<div class="panel panel-default post-panel">
			    	<%= link_to "http://www.codecademy.com/learn/learn-rails", target: '_blank' do %>
			    	<div class="panel-body post-panel-body learn-info">
			   			Codecademy
			   	  </div>
			   	  <% end %>
					</div>
			   	<div class="panel panel-default post-panel">
			    	<%= link_to "http://cli.learncodethehardway.org/book/", target: '_blank' do %>
			    	<div class="panel-body post-panel-body learn-info">
			   			Command Line Crash Course
					  </div>
					  <% end %>
					</div>
			  </div>
	    </div>
			<!--Tutorials tab pane-->
	    <div role="tabpanel" class="tab-pane" id="tutorials">
